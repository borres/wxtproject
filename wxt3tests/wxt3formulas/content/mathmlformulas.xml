<formulas>

	<formula id="f31" type="mathml">
	<subtext>Simple sum</subtext>
	<value><![CDATA[<math display="inline" xmlns="http://www.w3.org/1998/Math/MathML">
                <mrow>
                  <mrow>
                    <mo stretchy="false">∑</mo>
                    <mi>f</mi>
                  </mrow>
                  <mrow>
                    <mo stretchy="false">(</mo>
                    <mi>x</mi>
                    <mo stretchy="false">)</mo>
                  </mrow>
                </mrow>
            </math>]]>
	</value>
	</formula>	
	
	<formula id="f32" type="mathml">
	<subtext>Bezier from mathml</subtext>
	<value><![CDATA[<math display="inline"  xmlns="http://www.w3.org/1998/Math/MathML">
                <mrow>
                  <mi>B</mi>
                  <mrow>
                    <mrow>
                      <mo stretchy="false">(</mo>
                      <mi>t</mi>
                      <mo stretchy="false">)</mo>
                    </mrow>
                    <mo stretchy="false">=</mo>
                    <mrow>
                      <munderover>
                        <mo stretchy="false">∑</mo>
                        <mrow>
                          <mi>i</mi>
                          <mo stretchy="false">=</mo>
                          <mn>0</mn>
                        </mrow>
                        <mi>n</mi>
                      </munderover>
                      <msubsup>
                        <mi>B</mi>
                        <mi>i</mi>
                        <mi>n</mi>
                      </msubsup>
                    </mrow>
                  </mrow>
                  <mrow>
                    <msup>
                      <mi>t</mi>
                      <mi>i</mi>
                    </msup>
                    <msup>
                      <mrow>
                        <mo stretchy="false">(</mo>
                        <mrow>
                          <mn>1</mn>
                          <mo stretchy="false">−</mo>
                          <mi>t</mi>
                        </mrow>
                        <mo stretchy="false">)</mo>
                      </mrow>
                      <mrow>
                        <mo stretchy="false">(</mo>
                        <mrow>
                          <mi>n</mi>
                          <mo stretchy="false">−</mo>
                          <mi>i</mi>
                        </mrow>
                        <mo stretchy="false">)</mo>
                      </mrow>
                    </msup>
                    <msub>
                      <mi>P</mi>
                      <mi>i</mi>
                    </msub>
                  </mrow>
                </mrow>
            </math>]]>
	</value>
	</formula>	
</formulas>