<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
<head>
	<meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
	<title>test</title>
	<meta content="WXT" name="keywords"/>
	<meta content="wxt3" name="GENERATOR"/>
	
	<link href="templatestyle.css" rel="stylesheet"  type="text/css"/>
	
	<link rel="STYLESHEET" href="http://www.it.hiof.no/~borres/allround/stdstyles/wxtstyle.css"/>
	<script src="http://www.it.hiof.no/~borres/allround/jquery.js" type="text/javascript"> </script>
	<script src="http://www.it.hiof.no/~borres/allround/stdjs/wxtjs.js" type="text/javascript"> </script>
	
    <script src="http://www.it.hiof.no/~borres/allround/google-code-prettify/src/prettify.js" type="text/javascript"> </script>
    <script src="http://www.it.hiof.no/~borres/allround/google-code-prettify/src/lang-css.js" type="text/javascript"> </script>
    <script src="http://www.it.hiof.no/~borres/allround/google-code-prettify/src/lang-sql.js" type="text/javascript"> </script>
	<script src="http://www.it.hiof.no/~borres/allround/google-code-prettify/src/lang-ml.js" type="text/javascript"> </script>


</head>

<body>



<!-- logo -->
<div id="logo"> 
	<a href="http://www.it.hiof.no"> <img alt="logo" src="logo.png" style="border:none" /> </a> 
</div>
<!-- banner -->
<div id="banner">
  <p class="topright">Østfold University College</p>
  <p class="sitename">Test package for WXT</p>
</div>

<div id="all">


<!-- left menu -->
<div class="menu" id="menu">
	<?_wxt modulemenu summary="yes" books="all"?>
</div>



<!-- content -->
<div class="content">
	<div class="collectable">
		<h1>Wines from <span><?_wxt fragment id="_name"?></span></h1>
        
        
        <div style="float:right;width:300px">
        <?_wxt importwiki xpath="//img[contains(@src,'200px') and contains(@src,'Civil')]" usecopy="yes"?>
        </div>

        
        <div>
        <?_wxt importtxt parse="yes"?>
        </div>
        
        <h3>Some good red wines from <?_wxt fragment id="_name"?></h3>
        <div>
        <?_wxt importxml xpath="//div[@class='db']" ?>
        </div>        
        

	</div>
</div><!-- content -->


</div><!-- all -->

<!-- footer -->
<div class="footer">
</div><!-- eof footer -->



</body>
</html>