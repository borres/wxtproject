	<formula id="f4" type="latex">
	<value><![CDATA[B(t)=\sum_{i=0}^{n}B_{i}^{n}t^{i}(1-t)^{n-i}P_{_{i}}]]></value>
	</formula> 
	
	<formula id="f5a" type="latex">
	<subtext>Surface normals</subtext>
	<value><![CDATA[N_{x} (\phi ,\beta )=\frac{1}{r_{x}} cos^{2-n1} (\phi)cos^{2-n2}(\beta)]]></value>
	</formula>
	
	<formula id="f5b" type="latex">
	<subtext>Surface normals</subtext>
	<value><![CDATA[N_{y} (\phi ,\beta )=\frac{1}{r_{y}} cos^{2-n1} (\phi)sin^{2-n2}(\beta)]]></value>
	</formula>
	
	<formula id="f5c" type="latex">
	<subtext>Surface normals</subtext>
	<value><![CDATA[N_{x} (\phi ,\beta )=\frac{1}{r_{x}} sin^{2-n1} (\phi)]]></value>
	</formula>

