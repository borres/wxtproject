<?xml version="1.0" encoding="utf-8"?>
<data>
<div class="content">    
<h1>WIKIPEDIA import</h1>

<hr/>
<div>Source:</div>
<div><?_wxt expand location="exp_importwikipedia.xml" 
             sourcelocation="importwikipedia.xml" code="xmlcode"?>
</div>
<hr/>

<div>
	<?_wxt moduletoc tags="h1,h2,h3" cols="1"?>
</div>
<hr/>
<p>
	Testing import av WIKIPEDIA fragments
</p>
<hr/>
<div>
	<h1>Tigere</h1>
	<?_wxt importwiki keeprefs="yes" 
			 location="http://no.wikipedia.org/wiki/Tiger" 
			 dpath="//Underarter"?>
</div>
<hr/>
<div>
	<h1>Pumaer</h1>
	<?_wxt importwiki  keeprefs="no"
			 location="http://en.wikipedia.org/wiki/Cougar" 
			 dpath="//Hunting and diet"?>
</div>
<hr/>
<div>
	<h1>Pitcairn Islands</h1>
	<?_wxt importwiki  keeprefs="yes"
			 location="http://en.wikipedia.org/wiki/Pitcairn_Islands" 
			 xpath="//table[@class='infobox geography vcard']"?>
</div>
</div>
</data>